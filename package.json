{
  "name": "@cloro-dev/response-parser",
  "version": "0.3.1",
  "type": "module",
  "description": "Parse and render AI model responses from ChatGPT, Gemini, Perplexity, and more",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "cloro",
    "chatgpt",
    "gemini",
    "perplexity",
    "copilot",
    "html-parser",
    "typescript",
    "framework-agnostic"
  ],
  "author": "cloro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloro/response-parser.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.54.0",
    "husky": "^9.1.7",
    "rollup": "^4.6.1",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
